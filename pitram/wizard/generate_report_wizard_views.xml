<odoo>
    <data>
        <record id="form_generate_report_wizard" model="ir.ui.view">
            <field name="name">Form Generate Report Wizard</field>
            <field name="model">generate.report.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="from_date" />
                        </group>
                        <group>
                            <field name="to_date" />
                        </group>
                    </group>
                    <footer>
                        <button string="PDF export" name="pdf_export" type="object" data-hotkey="q" />
                        <button string="Call action" name="%(pitram.action_generate_report_wizard)d" type="action" data-hotkey="w" />
                        <button string="Cancel" class="btn btn-secondary" special="cancel" data-hotkey="x"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_generate_report_wizard" model="ir.actions.act_window">
            <field name="name">Action Generate Report Wizard</field>
            <field name="res_model">generate.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <menuitem
            action="action_generate_report_wizard"
            parent="fleet.fleet_vehicles"
            name="Generate Report"
            id="menu_generate_report_wizard"
            sequence="1"
        />
    </data>
</odoo>