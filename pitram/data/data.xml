<odoo>
    <data noupdate="1">
        <record id="sequence_check_sheet" model="ir.sequence">
            <field name="name">Check Sheet</field>
            <field name="code">check.sheet</field>
            <field name="active">TRUE</field>
            <field name="padding">3</field>
            <field name="prefix">CH/%(year)s/%(month)s/%(day)s.</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>