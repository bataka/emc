<odoo>
    <data>
        <record id="tree_diagnosis_history" model="ir.ui.view">
            <field name="name">Tree Diagnosis History</field>
            <field name="model">diagnosis.history</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="vehicle_id" />
                    <field name="diagnosis_date" />
                    <field name="diagnosis_next_date" />
                </tree>
            </field>
        </record>

        <record id="action_diagnosis_history1" model="ir.actions.act_window">
            <field name="name">Action Diagnosis History</field>
            <field name="res_model">diagnosis.history</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem
            parent="fleet.fleet_vehicles"
            action="action_diagnosis_history1"
            name="Diagnosis History"
            id="menu_diagnosis_history"
            sequence="99" 
        />

    </data>
</odoo>