<odoo>
    <data noupdate="1">
        <record id="fleet_service_diagnosis_alert_cron" model="ir.cron">
            <field name="name">Fleet Service Diagnosis Alert Cron</field>
            <field name="model_id" ref="model_diagnosis_history" />
            <field name="state">code</field>
            <field name="code">model.run_diagnosis_alert()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>
    </data>
</odoo>