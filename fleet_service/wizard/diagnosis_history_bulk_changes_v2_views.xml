<odoo>
    <record id="view_diagnosis_history_bulk_changes_v2" model="ir.ui.view">
        <field name="name">Diagnosis History Bulk Changes V2</field>
        <field name="model">diagnosis.history.bulk.changes.v2</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="from_template_id" />
                        </group>
                    </group>
                    <notebook>
                        <page string="Diagnosis History">
                            <field name="line_ids" />
                        </page>
                    </notebook>
                </sheet>
                <footer>
                    <button string="Convert" name="do_action" type="object" />
                </footer>
            </form>
        </field>
    </record>

